import java.util.Scanner;
  class Main {
    public static void main(String[] args) {
      Scanner entrada = new Scanner(System.in);

     
  int numero;
  int primeiro = 0;
  int segundo = 1;
  

  System.out.print("Digite o valor de número para que imprima os primeiros números da sequência de Fibonacci: ");
  numero = entrada.nextInt();

if(numero <= 0){
  System.out.println("Insira um valor válido(numero tem que ser maior que 0)");

}else{
 System.out.print("os primeiros" + numero + " números da sequência de Fibonacci são: ");
  
  for(int i = 1; i <= numero; i++){
    System.out.println(primeiro + " ");
    int proximo = primeiro + segundo;
    primeiro = segundo;
    segundo = proximo;
   }
}
  
  
}


}
  
