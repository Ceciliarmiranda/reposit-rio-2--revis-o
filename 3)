class Main {
  public static void main(String[] args) {

    int i = 0;
    do{
      System.out.println(i);
      i++;
    
    }while(i < 50);


    int i;
    for (i = 1; i <= 50; i++) {
    System.out.println(i + "\n");


      int i = 0;
      while (i < 50) {
        System.out.println(i + "\n");
        i++;
    
}
}
